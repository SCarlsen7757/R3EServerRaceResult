          "dataset": "r3e_racing",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH RankedIncidents AS (\r\n    SELECT\r\n        d.name,\r\n        i.session_id,\r\n        COUNT(*) AS incident_count,\r\n        ROW_NUMBER() OVER (\r\n            PARTITION BY d.name\r\n            ORDER BY COUNT(*) DESC\r\n        ) AS rank\r\n    FROM race_stats.incidents i\r\n    JOIN race_stats.sessions s\r\n        ON s.id = i.session_id\r\n    JOIN race_stats.events e\r\n        ON e.id = s.event_id\r\n    JOIN race_stats.drivers d\r\n        ON i.driver_id = d.id\r\n    WHERE\r\n        i.incident_type = 'NotServingPenalty'\r\n        AND e.event_date BETWEEN $__timeFrom() AND $__timeTo()\r\n    GROUP BY\r\n        d.name,\r\n        i.session_id\r\n)\r\nSELECT\r\n    name AS driver_name,\r\n    incident_count\r\nFROM RankedIncidents\r\nWHERE rank = 1\r\nORDER BY incident_count DESC\r\nLIMIT 5;",
          "refId": "A",
