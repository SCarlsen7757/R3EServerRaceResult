        {
          "dataset": "r3e_racing",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n    d.name AS driver_name,\r\n    AVG(session_incidents.total_incident_points) AS avg_incident_points\r\nFROM (\r\n    SELECT\r\n        i.driver_id,\r\n        i.session_id,\r\n        SUM(i.incident_points) AS total_incident_points\r\n    FROM race_stats.incidents i\r\n    JOIN race_stats.sessions s\r\n        ON s.id = i.session_id\r\n    JOIN race_stats.events e\r\n        ON e.id = s.event_id\r\n    WHERE\r\n        e.event_date BETWEEN $__timeFrom() AND $__timeTo()\r\n    GROUP BY\r\n        i.driver_id,\r\n        i.session_id\r\n) AS session_incidents\r\nJOIN race_stats.drivers d\r\n    ON session_incidents.driver_id = d.id\r\nGROUP BY\r\n    d.name\r\nORDER BY\r\n    avg_incident_points DESC;",
          "refId": "A",
